<?xml version="1.0" encoding="utf-8"?>
<com.vintech.mediaprotector.framework.WorkspaceLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/workspace"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.vintech.mediaprotector.MainActivity"
    tools:showIn="@layout/app_bar_main">

    <include
        android:id="@+id/photo_pick_layout"
        layout="@layout/photo_pick_layout" />

    <include
        android:id="@+id/photo_gallery_layout"
        layout="@layout/photo_gallery_layout" />

    <include
        android:id="@+id/video_pick_layout"
        layout="@layout/video_pick_layout" />

    <include
        android:id="@+id/video_gallery_layout"
        layout="@layout/video_gallery_layout" />

</com.vintech.mediaprotector.framework.WorkspaceLayout>
